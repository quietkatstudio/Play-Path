<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="com.controller.SongController" spacing="10" alignment="center">
    <!-- Song title label -->
    <Label fx:id="songTitleLabel" text="Song Title" />
    
    <!-- Artist label -->
    <Label fx:id="artistLabel" text="Artist" />
    
    <!-- Genre label -->
    <Label fx:id="genreLabel" text="Genre" />
    
    <!-- Duration label -->
    <Label fx:id="durationLabel" text="Duration" />
    
    <!-- Tempo label -->
    <Label fx:id="tempoLabel" text="Tempo" />
    
    <!-- Play button -->
    <Button fx:id="playButton" text="Play" />
</VBox>